typedef enum{
  def = 40,
    red    = 41, 
    green = 42, 
    blue   = 44, 
} bgcolor;
typedef enum{
    RED   = 31,
    GREEN = 32,
    BLUE  = 34, 
} fgcolor;
int mt_clrscr(void);
int mt_gotoXY(int x, int y); 
int mt_getscreensize(int *rows, int *cols);
int setfgcolor(fgcolor fg);
int setbgcolor(bgcolor bg);   
